with "formatted_output_ng.gpr";

project Formatted_Output_NG_Demo is

   for Source_Dirs use ("demo");
   for Object_Dir use "obj";
   for Main use ("integer_output_demo.adb");

   package Builder is
      for Default_Switches ("ada") use ("-s", "-g");
   end Builder;

   package Compiler is
      for Default_Switches ("ada") use ("-g");
   end Compiler;

   package Linker is
      for Default_Switches ("ada") use ("-g");
   end Linker;

end Formatted_Output_NG_Demo;

